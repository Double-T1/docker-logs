[
  {
    "name": "logger",
    "image": "${AWS_ACCOUNT_ID}.dkr.ecr.${AWS_REGION}.amazonaws.com/${PROJECT_FULL_NAME}-logger:latest",
    "cpu": 256,
    "memory": 512,
    "portMappings": [
      {
        "name": "logger-8000-tcp",
        "containerPort": 8000,
        "hostPort": 8000,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "readonlyRootFilesystem": false,
    "environment": [
      {
        "name": "ENV",
        "value": "${ENVIRONMENT}"
      },
      {
        "name": "AWS_REGION",
        "value": "${AWS_REGION}"
      },
      {
        "name": "LOG_LEVEL",
        "value": "DEBUG"
      }
    ],
    "secrets": [],
    "mountPoints": [],
    "volumesFrom": [],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/${PROJECT_FULL_NAME}-logger",
        "awslogs-create-group": "true",
        "awslogs-region": "${AWS_REGION}",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "systemControls": []
  }
]
